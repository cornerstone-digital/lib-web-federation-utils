{
  "name": "@vf/federated-web-cli",
  "version": "1.0.0-alpha.1",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "bin": {
    "vf-federated": "./dist/src/index.js"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "compile:types": "tsc --project tsconfig.json --outDir dist --declaration --emitDeclarationOnly",
    "postbuild": "npx ncp ./package.json dist/package.json && yarn compile:types",
    "preyalc:publish": "yarn build",
    "yalc:publish": "yalc publish --push",
    "cli": "ts-node src/index.ts"
  },
  "dependencies": {
    "commander": "^9.2.0",
    "esbuild": "^0.14.38",
    "inquirer": "^8.2.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.6",
    "webpack": "^5.72.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime-corejs3": "^7.17.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "babelrc-rollup": "^3.0.0",
    "brotli-webpack-plugin": "^1.1.0",
    "https-browserify": "^1.0.0",
    "rollup": "^2.71.1",
    "rollup-plugin-babel": "^4.4.0",
    "stream-http": "^3.2.0"
  }
}
