{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es6"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "baseUrl": "./",
    "typeRoots": [
      "./",
      "./node_modules/@types"
    ],
    "types": [],
    "noEmit": false,
    "forceConsistentCasingInFileNames": true
  }
}
