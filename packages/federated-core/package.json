{
  "name": "@vf/federated-core",
  "version": "0.1.0-alpha.9",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "postbuild": "yarn clean:dist && yarn copy:shared-modules",
    "clean:dist": "rimraf dist/jest*",
    "copy:shared-modules": "ncp src/shared-modules dist/src/shared-modules",
    "lint": "eslint . --ext .js,.ts,.tsx --fix",
    "test": "jest",
    "test:watch": "yarn test --watchAll",
    "test:cc": "yarn test --coverage",
    "test:cc:watch": "yarn test:cc --watchAll"
  },
  "dependencies": {
    "@datadog/browser-logs": "^4.11.1",
    "@swc/helpers": "~0.3.3",
    "systemjs": "^6.12.1",
    "ts-node": "^10.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/systemjs": "^6.1.1",
    "jest-fetch-mock": "^3.0.3",
    "ncp": "^2.0.0",
    "typescript": "^4.7.2"
  }
}
