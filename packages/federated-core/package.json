{
    "name": "@vf/federated-core",
    "version": "0.11.0",
    "type": "commonjs",
    "main": "./dist/federated-core.cjs.js",
    "module": "./dist/federated-core.es.js",
    "exports": {
        ".": {
            "require": "./dist/federated-core.cjs.js",
            "default": "./dist/federated-core.es.js"
        }
    },
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "vite build",
        "postbuild": "yarn clean:dist",
        "clean:dist": "rimraf dist/jest*",
        "lint": "eslint . --ext .ts,.tsx --fix",
        "test": "jest --config jest.config.ts",
        "test:watch": "yarn test --watchAll",
        "test:cc": "yarn test --coverage",
        "test:cc:watch": "yarn test:cc --watchAll"
    },
    "dependencies": {
        "@datadog/browser-logs": "^4.11.1",
        "@swc/helpers": "~0.3.3",
        "fs-extra": "^10.1.0",
        "systemjs": "^6.12.1",
        "ts-node": "^10.8.1"
    },
    "devDependencies": {
        "@rollup/plugin-typescript": "^8.3.2",
        "@types/systemjs": "^6.1.1",
        "jest-fetch-mock": "^3.0.3",
        "ncp": "^2.0.0",
        "typescript": "^4.7.2",
        "vite": "2.9.6",
        "vite-plugin-dts": "^1.2.1"
    }
}
