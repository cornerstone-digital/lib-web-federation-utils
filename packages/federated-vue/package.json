{
    "name": "@vf/federated-vue",
    "version": "0.3.2",
    "main": "dist/federated-vue.cjs.js",
    "module": "dist/federated-vue.es.js",
    "exports": {
        ".": {
            "require": "./dist/federated-vue.cjs.js",
            "default": "./dist/federated-vue.es.js"
        }
    },
    "types": "dist/src/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "vite build",
        "postbuild": "rimraf dist/jest*",
        "lint": "eslint . --ext .js,.ts,.tsx --fix",
        "test": "jest --passWithNoTests --config ./jest.config.ts",
        "test:watch": "yarn test --watchAll",
        "test:cc": "yarn test --coverage",
        "test:cc:watch": "yarn test:cc --watchAll"
    },
    "dependencies": {
        "@vf/federated-core": "^0.5.0",
        "ajv": "^8.11.0",
        "systemjs": "^6.12.1",
        "vue": "^3.2.25"
    },
    "devDependencies": {
        "@types/systemjs": "^6.1.1",
        "@vitejs/plugin-vue": "^2.3.3",
        "rimraf": "^3.0.2",
        "typescript": "^4.5.4",
        "vite": "^2.9.13",
        "vite-plugin-dts": "^1.2.1",
        "vue-tsc": "^0.34.7"
    }
}
